pipeline {
    agent any
    stages {
        stage('Build') {
           
              steps{
                
            sh 'echo Hello Build'
                // "echo This is build stage"
              }
        }

        stage('test') {
        
              steps{
                
            sh 'echo Hello test'
                // "echo This is test stage"
              }
        }
    

    stage('Deploy') {
            
              steps{
                
            sh 'echo Hello deploy'
                // "echo This is Deploy stage"
              }
        }
}
        }
    