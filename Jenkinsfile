pipeline {
    agent any
    stages {
        stage('Build') {
            steps {
              steps{
                
            sh "echo Hello Build"
                // "echo This is build stage"
              }
        }

        stage('test') {
            steps {
              steps{
                
            sh "echo Hello test"
                // "echo This is test stage"
              }
        }
    }

    stage('Deploy') {
            steps {
              steps{
                
            sh "echo Hello deploy"
                // "echo This is Deploy stage"
              }
        }
}
        }
    }
}